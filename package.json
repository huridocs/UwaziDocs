{
  "name": "Uwazi",
  "version": "1.4.1",
  "description": "Uwazi is a free, open-source solution for organising, analysing and publishing your documents.",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:huridocs/uwazidocs.git"
  },
  "keywords": [
    "react"
  ],
  "author": "huridocs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huridocs/uwazidocs/issues"
  },
  "homepage": "https://www.uwazi.io/",
  "jest": {
    "projects": [
      "app/jest.client.config.js",
      "app/jest.server.config.js"
    ]
  },
  "scripts": {
    "production-build": "NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "run-production": "export NODE_ENV=production; node server.js",
    "dev-server": "./node_modules/.bin/nodemon --ignore 'app/dist/*' --watch 'app/api' --watch 'app/shared' --watch 'app/react/ServerRouter.js' --watch 'app/react/App/Root.js'",
    "webpack-server": "node ./webpack/webpack.server.js",
    "hot": "export HOT=true;npm run dev-server & npm run webpack-server",
    "hot-inspect": "export HOT=true;yarn dev-server --inspect & npm run webpack-server",
    "download-mongo-prebuilt": "node ./database/download_mongo_prebuilt.js",
    "postinstall": "node -e \"process.env.NODE_ENV != 'production' && process.exit(1)\" || npm run download-mongo-prebuilt",
    "reindex": "node run.js ./database/reindex_elastic.js",
    "health-check": "node run.js ./database/health_check.js",
    "blank-state": "./database/blank_state.sh; yarn migrate; yarn reindex",
    "admin-user": "./database/admin_user.sh",
    "fixtures": "cd uwazi-fixtures; ./restore.sh; yarn reindex",
    "add-migration": "./node_modules/.bin/plop --plopfile ./app/api/migrations/plopfile.js migration",
    "migrate": "node run.js ./app/api/migrations/migrate.js",
    "test-debug": "node --inspect ./node_modules/.bin/jest --watch --no-cache -i",
    "test": "node ./node_modules/.bin/jest",
    "e2e": "jest --projects nightmare/jest.e2e.config.js -i --verbose true",
    "e2e-show": "export SHOW_E2E=1; yarn e2e",
    "e2e-restore-fixtures": "cd nightmare/fixtures;./restore.sh",
    "eslint": "./node_modules/.bin/eslint app --quiet"
  },
