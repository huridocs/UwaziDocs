{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "retainLines": true,
  "env": {
    "production": {
      "plugins": [
        "babel-plugin-transform-react-remove-prop-types",
        "@babel/plugin-transform-react-inline-elements"
      ]
    },
    "debug": {
      "sourceMaps": "inline",
      "retainLines": true
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ]
}
